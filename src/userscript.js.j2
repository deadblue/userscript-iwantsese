'use strict';

{% for h in handlers %}
const handler_{{h.name}} = () => {{ h.code }};
{% endfor %}

{% raw %}
const _handlerMap = new Map([
{% endraw %}
{% for h in handlers %}
{% for d in h.domains %}
    ['{{ d }}', handler_{{h.name}}],
{% endfor %}
{% endfor %}
{% raw %}
]);

(function(){
    const handler = _handlerMap.get(location.hostname);
    if (handler !== undefined) { handler(); }
})();
{% endraw %}